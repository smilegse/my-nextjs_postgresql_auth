generator client {
  provider = "prisma-cnplient-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  userName  String   @unique
  password  String
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
}
